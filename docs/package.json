{
  "name": "docs-app",
  "version": "0.0.0",
  "private": true,
  "packageManager": "bun@1.3.0",
  "engines": {
    "bun": ">=1.3.0",
    "node": ">=20.19.0"
  },
  "scripts": {
    "copy-schemas": "mkdir -p src/schemas && cp -r ../schemas/*.json src/schemas/ 2>/dev/null || true",
    "prebuild": "bun run copy-schemas",
    "build": "next build",
    "predev": "bun run copy-schemas",
    "dev": "next dev --turbo -p 5006",
    "start": "next start",
    "postinstall": "fumadocs-mdx",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out dist src/schemas",
    "clean:deps": "rm -rf node_modules bun.lockb",
    "reinstall": "bun run clean:deps && bun install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:openapi": "bun ./scripts/generate-openapi.ts",
    "update:deps": "updates -Gu"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "13.7.0",
    "@radix-ui/react-dialog": "1.1.14",
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/react-select": "2.2.5",
    "@radix-ui/react-separator": "1.1.7",
    "@radix-ui/react-slot": "1.2.3",
    "@radix-ui/react-tooltip": "1.2.7",
    "@radix-ui/react-use-controllable-state": "1.2.2",
    "@scalar/api-client-react": "1.3.27",
    "@shikijs/transformers": "3.9.2",
    "@storybook/nextjs-vite": "9.1.1",
    "@tabler/icons-react": "3.34.1",
    "@tsparticles/engine": "3.9.1",
    "@tsparticles/react": "3.0.0",
    "@tsparticles/slim": "3.9.1",
    "@vercel/analytics": "1.5.0",
    "class-variance-authority": "0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.1.1",
    "embla-carousel-react": "8.6.0",
    "fumadocs-core": "15.6.9",
    "fumadocs-mdx": "12.0.0",
    "fumadocs-openapi": "9.1.8",
    "fumadocs-ui": "15.6.9",
    "geist": "1.4.2",
    "lucide-react": "0.544.0",
    "mermaid": "11.9.0",
    "mermaid-isomorphic": "3.0.4",
    "motion": "12.23.12",
    "next": "15.4.5",
    "next-themes": "^0.4.6",
    "openmeteo": "1.2.0",
    "radix-ui": "^1.4.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-markdown": "10.1.0",
    "react-medium-image-zoom": "5.3.0",
    "rehype-mermaid": "3.0.0",
    "remark-gfm": "4.0.1",
    "shiki": "3.9.2",
    "sonner": "^2.0.7",
    "svg-pan-zoom": "3.6.2",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "3.1.1"
  },
  "devDependencies": {
    "@emotion/is-prop-valid": "1.3.1",
    "@storybook/addon-themes": "9.1.1",
    "@storybook/nextjs": "9.1.1",
    "@tailwindcss/postcss": "4.1.11",
    "@types/mdx": "2.0.13",
    "@types/node": "24.2.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "postcss": "8.5.6",
    "storybook": "9.1.1",
    "tailwindcss": "4.1.11",
    "tw-animate-css": "1.3.6"
  }
}
