@baseUrl = http://localhost:5001/api/v0
@agentId = agent_translator
@taskId = translate

### Execute agent synchronously
# @name runAgentSync
POST {{baseUrl}}/agents/{{agentId}}/executions/sync
Content-Type: application/json
Accept: application/json

{
  "prompt": "Do a new translate",
  "with": {
    "text": "Ol√°, tudo bom?",
    "from": "Portuguese",
    "to": "English"
  }
}

### Execute agent synchronously
# @name runTaskSync
POST {{baseUrl}}/tasks/{{taskId}}/executions/sync
Content-Type: application/json
Accept: application/json

{
  "with": {
    "languages": "portugues, english and spanish"
  }
}
