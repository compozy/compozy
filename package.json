{
  "name": "compozy-examples",
  "version": "0.1.0",
  "description": "Compozy runtime examples and tools",
  "private": true,
  "type": "module",
  "workspaces": [
    "examples/**",
    "docs"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "storybook": "turbo run storybook",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,js,tsx,json,md}\"",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "clean:deps": "rm -rf node_modules bun.lockb",
    "reinstall": "bun run clean:deps && bun install"
  },
  "devDependencies": {
    "@storybook/nextjs": "9.0.15",
    "@types/bun": "1.2.18",
    "oxlint": "1.6.0",
    "prettier": "3.6.2",
    "prettier-plugin-multiline-arrays": "4.0.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-sh": "0.18.0",
    "turbo": "2.5.4",
    "typescript": "5.8.3",
    "vite": "^7.0.4"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "cmdk": "^1.1.1",
    "mermaid": "^11.8.1",
    "next-themes": "^0.4.6",
    "openmeteo": "1.2.0",
    "react-markdown": "10.1.0",
    "rehype-mermaid": "^3.0.0",
    "remark-gfm": "4.0.1",
    "svg-pan-zoom": "^3.6.2"
  },
  "packageManager": "bun@1.2.18"
}
