{
  "name": "compozy-examples",
  "version": "0.1.0",
  "description": "Compozy runtime examples and tools",
  "private": true,
  "type": "module",
  "workspaces": [
    "examples/**",
    "docs",
    "tools/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "storybook": "turbo run storybook",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,js,tsx,json,md}\"",
    "format:yaml": "prettier --write \"**/*.{yml,yaml}\"",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "clean:deps": "rm -rf node_modules bun.lockb",
    "reinstall": "bun run clean:deps && bun install"
  },
  "devDependencies": {
    "@storybook/nextjs": "9.0.15",
    "@types/bun": "1.2.18",
    "oxlint": "1.6.0",
    "prettier": "3.6.2",
    "prettier-plugin-multiline-arrays": "4.0.3",
    "prettier-plugin-organize-imports": "4.1.0",
    "prettier-plugin-sh": "0.18.0",
    "turbo": "2.5.4",
    "typescript": "5.8.3",
    "vite": "^7.0.4"
  },
  "packageManager": "bun@1.2.18",
  "dependencies": {
    "lucide-static": "^0.525.0"
  }
}
