{
  "$defs": {
    "EmbedderCacheConfig": {
      "additionalProperties": false,
      "description": "EmbedderCacheConfig toggles client-side embedding caches.",
      "properties": {
        "enabled": {
          "type": "boolean"
        },
        "size": {
          "type": "integer"
        }
      },
      "type": "object"
    },
    "EmbedderRuntimeConfig": {
      "additionalProperties": false,
      "description": "EmbedderRuntimeConfig captures runtime tuning options for an embedder client.",
      "properties": {
        "batch_size": {
          "type": "integer"
        },
        "cache": {
          "$ref": "#/$defs/EmbedderCacheConfig"
        },
        "dimension": {
          "type": "integer"
        },
        "retry": {
          "type": "object"
        },
        "strip_newlines": {
          "type": "boolean"
        }
      },
      "type": "object"
    }
  },
  "$id": "embedder.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "description": "EmbedderConfig describes an embedding provider used during knowledge ingestion.",
  "properties": {
    "api_key": {
      "type": "string"
    },
    "config": {
      "$ref": "#/$defs/EmbedderRuntimeConfig"
    },
    "id": {
      "type": "string"
    },
    "model": {
      "type": "string"
    },
    "provider": {
      "type": "string"
    }
  },
  "title": "Embedder Configuration",
  "type": "object",
  "yamlCompatible": true
}
